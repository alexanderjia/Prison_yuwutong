<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mAttr="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <com.starlight.mobile.android.lib.view.CusHeadView
        android:id="@+id/cut_photo_layout_ch_head"
        style="@style/album_head_style"
        mAttr:chHead_rightText="@string/lib_confirm" />

    <com.starlight.mobile.android.lib.view.CutPhotoView
        android:id="@+id/cut_photo_layout_cp_cut"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>