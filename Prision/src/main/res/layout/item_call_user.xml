<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/item_call_user_iv_1"
        android:layout_width="534dp"
        android:layout_height="333dp"
        android:background="#B7B7B7"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
    />

    <ImageView
        android:id="@+id/item_call_user_iv_2"
        android:layout_width="534dp"
        android:layout_height="333dp"
        android:layout_marginTop="20dp"
        android:background="#B7B7B7"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="@+id/item_call_user_iv_1"
        app:layout_constraintStart_toStartOf="@+id/item_call_user_iv_1"
        app:layout_constraintTop_toBottomOf="@id/item_call_user_iv_1"/>

</android.support.constraint.ConstraintLayout>